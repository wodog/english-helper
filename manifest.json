{
  "manifest_version": 2,
  "name": "english-helper",
  "description": "english-helper",
  "version": "1.0",
  "permissions": [
    "http://www.beikaodi.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js": [
				"libs/jquery-1.11.3.min.js", 
				"libs/bootstrap-3.3.5-dist/js/bootstrap.min.js", 
				"src/main.js"
			],
			"css": [
				"libs/bootstrap-3.3.5-dist/css/bootstrap.min.css"
			],
			"run_at": "document_end",
			"all_frames": true
		}
	]
}